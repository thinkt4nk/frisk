				</form>
			</div>
		</div>
    

<!-- TEMPLATES -->
<script type="javascript/template" id="task-template">
		<div class="frisk-state" data-id="${id}">
			<div class="title">${title}</div>
			<div class="remove"></div>
		</div>
</script>
